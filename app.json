{
  "expo": {
    "name": "minefield",
    "slug": "minefield",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-custom-png.png",
    "splash": {
      "image": "./assets/splash-custom.png",
      "resizeMode": "contain",
      "backgroundColor": "#a2a3a3"

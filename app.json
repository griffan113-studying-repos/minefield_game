      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-custom-png.png",
        "backgroundColor": "#a2a3a3"
      },
      "package": "com.eliel.minefieldgame"
    },
    "web": {
      "favicon": "./assets/icon-custom-png.png"
    }
  }
